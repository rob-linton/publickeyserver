{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Sure Drop .NET Core Launch (console)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_suredrop",
			"program": "${workspaceFolder}/suredrop/bin/Debug/net8.0/suredrop.dll",
			"args": [],
			"cwd": "${workspaceFolder}/suredrop",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			// Use IntelliSense to find out which attributes exist for C# debugging
			// Use hover for the description of the existing attributes
			// For further information visit https://github.com/dotnet/vscode-csharp/blob/main/debugger-launchjson.md.
			"name": "Sure Drop .NET Core Launch (web)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_suredrop",
			// If you have changed target frameworks, make sure to update the program path.
			"program": "${workspaceFolder}/suredrop/bin/Debug/net8.0/suredrop.dll",
			"args": [],
			"cwd": "${workspaceFolder}/suredrop",
			"stopAtEntry": false,
			// Enable launching a web browser when ASP.NET Core starts. For more information: https://aka.ms/VSCode-CS-LaunchJson-WebBrowser
			"serverReadyAction": {
				"action": "openExternally",
				"pattern": "\\bNow listening on:\\s+(https?://\\S+)"
			},
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"sourceFileMap": {
				"/Views": "${workspaceFolder}/Views"
			}
		},
		{
			"name": "surepack (dev create)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_surepack",
			"program": "${workspaceFolder}/surepack/bin/Debug/net8.0/surepack.dll",
			"args": ["create", "-v", "3", "-p","password"],
			"cwd": "${workspaceFolder}/surepack/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "surepack (dev verify email)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_surepack",
			"program": "${workspaceFolder}/surepack/bin/Debug/net8.0/surepack.dll",
			"args": ["verify", "-v", "1", "-d", "localhost:5001", "-e", "rob.linton@mac.com"],
			"cwd": "${workspaceFolder}/surepack/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "surepack (dev create email)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_surepack",
			"program": "${workspaceFolder}/surepack/bin/Debug/net8.0/surepack.dll",
			"args": ["create", "-v", "1", "-d", "localhost:5001", "-p","password", "-e", "rob.linton@mac.com", "-t","VUFU5ZHU"],
			"cwd": "${workspaceFolder}/surepack/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "surepack (dev certify)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_surepack",
			"program": "${workspaceFolder}/surepack/bin/Debug/net8.0/surepack.dll",
			"args": ["certify","-v","1","-d", "localhost:5001", "-p", "password", "-a", "rob.linton@mac.com"],
			"cwd": "${workspaceFolder}/surepack/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "surepack (dev delete)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_surepack",
			"program": "${workspaceFolder}/surepack/bin/Debug/net8.0/surepack.dll",
			"args": ["delete","-v","1","-d", "localhost:5001", "-p", "password", "-a", "rob.linton@mac.com"],
			"cwd": "${workspaceFolder}/surepack/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "surepack (dev pack)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_surepack",
			"program": "${workspaceFolder}/surepack/bin/Debug/net8.0/surepack.dll",
			"args": ["pack","-v","1","-p","password", "-f", "earn-eligibility-contained.suredrop.org", "-a","earn-eligibility-contained.suredrop.org","-i","*.pdf", "-s","This is a subject","-m","This is a message","-o","Rob Linton.surepack"],
			"cwd": "~/Downloads",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "surepack (dev send)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_surepack",
			"program": "${workspaceFolder}/surepack/bin/Debug/net8.0/surepack.dll",
			"args": ["send","-v","1","-d", "localhost:5001", "-p","password"],
			"cwd": "${workspaceFolder}/surepack/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "surepack (dev receive)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_surepack",
			"program": "${workspaceFolder}/surepack/bin/Debug/net8.0/surepack.dll",
			"args": ["receive","-v","1","-d", "localhost:5001", "-p","password", "-f"],
			"cwd": "${workspaceFolder}/surepack/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "surepack (dev unpack)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_surepack",
			"program": "${workspaceFolder}/surepack/bin/Debug/net8.0/surepack.dll",
			"args": ["unpack","-v","1","-i","Rob Linton.surepack", "-p","password", "-d", "localhost:5001"],
			"cwd": "${workspaceFolder}/surepack/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "surepack (dev list)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_surepack",
			"program": "${workspaceFolder}/surepack/bin/Debug/net8.0/surepack.dll",
			"args": ["list","-v","0","-d", "localhost:5001"],
			"cwd": "${workspaceFolder}/surepack/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "surepack (About)", 
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_surepack",
			"program": "${workspaceFolder}/surepack/bin/Debug/net8.0/surepack.dll",
			"args": ["about"],
			"cwd": "${workspaceFolder}/surepack/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "surepack (Gui)", 
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_surepack",
			"program": "${workspaceFolder}/surepack/bin/Debug/net8.0/surepack.dll",
			"args": [],
			//"cwd": "${workspaceFolder}/surepack/test",
			"cwd": "~/Downloads/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": ".NET Core Attach",
			"type": "coreclr",
			"request": "attach"
		},
		
	]
}