{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Public Key Server .NET Core Launch (console)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_publickeyserver",
			"program": "${workspaceFolder}/publickeyserver/bin/Debug/net8.0/publickeyserver.dll",
			"args": [],
			"cwd": "${workspaceFolder}/publickeyserver",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			// Use IntelliSense to find out which attributes exist for C# debugging
			// Use hover for the description of the existing attributes
			// For further information visit https://github.com/dotnet/vscode-csharp/blob/main/debugger-launchjson.md.
			"name": "Public Key Server .NET Core Launch (web)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_publickeyserver",
			// If you have changed target frameworks, make sure to update the program path.
			"program": "${workspaceFolder}/publickeyserver/bin/Debug/net8.0/publickeyserver.dll",
			"args": [],
			"cwd": "${workspaceFolder}/publickeyserver",
			"stopAtEntry": false,
			// Enable launching a web browser when ASP.NET Core starts. For more information: https://aka.ms/VSCode-CS-LaunchJson-WebBrowser
			"serverReadyAction": {
				"action": "openExternally",
				"pattern": "\\bNow listening on:\\s+(https?://\\S+)"
			},
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"sourceFileMap": {
				"/Views": "${workspaceFolder}/Views"
			}
		},
		{
			"name": "JDoe (create)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_jdoe",
			"program": "${workspaceFolder}/jdoe/bin/Debug/net8.0/jdoe.dll",
			"args": ["create","-d", "localhost:5001", "-p","password"],
			"cwd": "${workspaceFolder}/jdoe",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "JDoe (verify)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_jdoe",
			"program": "${workspaceFolder}/jdoe/bin/Debug/net8.0/jdoe.dll",
			"args": ["verify","-d", "localhost:5001", "-p","password", "-a","tit-tickets-arguments.publickeyserver.org"],
			"cwd": "${workspaceFolder}/jdoe",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "JDoe (pack)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_jdoe",
			"program": "${workspaceFolder}/jdoe/bin/Debug/net8.0/jdoe.dll",
			"args": ["pack","-d", "localhost:5001", "-p","password", "-a","everything-developing-foods.publickeyserver.org","university-hospitality-he.publickeyserver.org","-i","*.jpg","-f","everything-developing-foods.publickeyserver.org"],
			"cwd": "${workspaceFolder}/jdoe",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": ".NET Core Attach",
			"type": "coreclr",
			"request": "attach"
		},
		
	]
}