{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Public Key Server .NET Core Launch (console)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_publickeyserver",
			"program": "${workspaceFolder}/publickeyserver/bin/Debug/net8.0/publickeyserver.dll",
			"args": [],
			"cwd": "${workspaceFolder}/publickeyserver",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			// Use IntelliSense to find out which attributes exist for C# debugging
			// Use hover for the description of the existing attributes
			// For further information visit https://github.com/dotnet/vscode-csharp/blob/main/debugger-launchjson.md.
			"name": "Public Key Server .NET Core Launch (web)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_publickeyserver",
			// If you have changed target frameworks, make sure to update the program path.
			"program": "${workspaceFolder}/publickeyserver/bin/Debug/net8.0/publickeyserver.dll",
			"args": [],
			"cwd": "${workspaceFolder}/publickeyserver",
			"stopAtEntry": false,
			// Enable launching a web browser when ASP.NET Core starts. For more information: https://aka.ms/VSCode-CS-LaunchJson-WebBrowser
			"serverReadyAction": {
				"action": "openExternally",
				"pattern": "\\bNow listening on:\\s+(https?://\\S+)"
			},
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"sourceFileMap": {
				"/Views": "${workspaceFolder}/Views"
			}
		},
		{
			"name": "deadrop (dev create)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_deadrop",
			"program": "${workspaceFolder}/deadrop/bin/Debug/net8.0/deadrop.dll",
			"args": ["create","-v","1","-d", "localhost:5001", "-p","password"],
			"cwd": "${workspaceFolder}/deadrop/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "deadrop (dev verify)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_deadrop",
			"program": "${workspaceFolder}/deadrop/bin/Debug/net8.0/deadrop.dll",
			"args": ["verify","-v","0","-d", "localhost:5001", "-p","password", "-a","britannica-rings-monitor.publickeyserver.org"],
			"cwd": "${workspaceFolder}/deadrop/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "deadrop (dev pack)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_deadrop",
			"program": "${workspaceFolder}/deadrop/bin/Debug/net8.0/deadrop.dll",
			"args": ["pack","-v","0","-d", "localhost:5001", "-p","password", "-o", "Rob Linton", "-a","expiration-obesity-aberdeen.publickeyserver.org","wedding-hereby-browser.publickeyserver.org","-i","*.jpg","-f","britannica-rings-monitor.publickeyserver.org"],
			"cwd": "${workspaceFolder}/deadrop/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "deadrop (dev send)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_deadrop",
			"program": "${workspaceFolder}/deadrop/bin/Debug/net8.0/deadrop.dll",
			"args": ["send","-v","1","-d", "localhost:5001", "-p","password","-i","Rob Linton.deadpack"],
			"cwd": "${workspaceFolder}/deadrop/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "deadrop (dev receive)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_deadrop",
			"program": "${workspaceFolder}/deadrop/bin/Debug/net8.0/deadrop.dll",
			"args": ["receive","-v","2","-d", "localhost:5001", "-p","password","-o","My.deadpack", "-i","BF4188889A12C134CAB48BEAF0672DE06F9CC45F3164FD117651CC3E88C891BA","-a","expiration-obesity-aberdeen.publickeyserver.org"],
			"cwd": "${workspaceFolder}/deadrop/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "deadrop (dev unpack)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_deadrop",
			"program": "${workspaceFolder}/deadrop/bin/Debug/net8.0/deadrop.dll",
			"args": ["unpack","-v","1","-i","Rob Linton","-d", "localhost:5001", "-p","password", "-a","expiration-obesity-aberdeen.publickeyserver.org", "-o","output"],
			"cwd": "${workspaceFolder}/deadrop/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "deadrop (dev list)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_deadrop",
			"program": "${workspaceFolder}/deadrop/bin/Debug/net8.0/deadrop.dll",
			"args": ["list","-v","1","-d", "localhost:5001", "-p","password"],
			"cwd": "${workspaceFolder}/deadrop/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "deadrop (create)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_deadrop",
			"program": "${workspaceFolder}/deadrop/bin/Debug/net8.0/deadrop.dll",
			"args": ["create","-v","0", "-p","password"],
			"cwd": "${workspaceFolder}/deadrop/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "deadrop (verify)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_deadrop",
			"program": "${workspaceFolder}/deadrop/bin/Debug/net8.0/deadrop.dll",
			"args": ["verify","-v","0","-p","password","-a","britannica-rings-monitor.publickeyserver.org"],
			"cwd": "${workspaceFolder}/deadrop/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "deadrop (pack)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_deadrop",
			"program": "${workspaceFolder}/deadrop/bin/Debug/net8.0/deadrop.dll",
			"args": ["pack","-v","0","-p","password","-a","expiration-obesity-aberdeen.publickeyserver.org","wedding-hereby-browser.publickeyserver.org","-i","Rob Linton - Speed Week 2023.jpg","-f","britannica-rings-monitor.publickeyserver.org"],
			"cwd": "${workspaceFolder}/deadrop/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
		},
		{
			"name": "deadrop (unpack)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_deadrop",
			"program": "${workspaceFolder}/deadrop/bin/Debug/net8.0/deadrop.dll",
			"args": ["unpack","-v","0","-i","deadrop.zip","-p","password","-a","expiration-obesity-aberdeen.publickeyserver.org", "-o","output"],
			"cwd": "${workspaceFolder}/deadrop/test",
			"stopAtEntry": false,
			"console":"integratedTerminal",
			"env": {
